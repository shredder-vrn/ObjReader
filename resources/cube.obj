# Вершины (8 точек)
v -1.0 -1.0 -1.0   # v0
v  1.0 -1.0 -1.0   # v1
v  1.0  1.0 -1.0   # v2
v -1.0  1.0 -1.0   # v3
v -1.0 -1.0  1.0   # v4
v  1.0 -1.0  1.0   # v5
v  1.0  1.0  1.0   # v6
v -1.0  1.0  1.0   # v7

# Нормали (по одной на каждую грань)
vn  0.0  0.0 -1.0   # нормаль к передней стороне
vn  0.0  0.0  1.0   # нормаль к задней стороне
vn  0.0 -1.0  0.0   # нормаль вниз
vn  0.0  1.0  0.0   # нормаль вверх
vn  1.0  0.0  0.0   # нормаль направо
vn -1.0  0.0  0.0   # нормаль налево

# Текстурные координаты (UV) — по 4 на грань (если текстурированный)
vt 0.0 0.0
vt 0.0 0.0
vt 1.0 0.0
vt 0.0 0.0




# Грани (каждая ссылается на вершины и нормали)
# Формат: f v/vt/vn

# Передняя грань (Z = -1)
f 1//1 2/2/1 3/3/1 4/4/1

# Задняя грань (Z = 1)
f 5/1/2 8/4/2 7/3/2 6/2/2

# Нижняя грань (Y = -1)
f 1/1/3 5/2/3 6/3/3 2/4/3

# Верхняя грань (Y = 1)
f 3/1/4 7/4/4 8/3/4 4/2/4

# Правая грань (X = 1)
f 2/1/5 6/2/5 7/3/5 3/4/5

# Левая грань (X = -1)
f 4/1/6 8/2/6 5/3/6 1/4/6
